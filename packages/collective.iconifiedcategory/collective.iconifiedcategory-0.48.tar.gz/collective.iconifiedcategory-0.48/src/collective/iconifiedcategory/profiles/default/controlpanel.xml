<?xml version="1.0"?>
<object
  name="portal_controlpanel"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="collective.iconifiedcategory"
  purge="False">

  <configlet
    title="Iconified Category"
    action_id="iconifiedcategory"
    appId="collective.iconifiedcategory"
    category="Products"
    condition_expr=""
    url_expr="string:${portal_url}/@@iconifiedcategory-controlpanel"
    icon_expr="string:$portal_url/maintenance_icon.png"
    visible="True"
    i18n:attributes="title">
      <permission>Manage portal</permission>
  </configlet>

</object>
