    async def {{ listener_item.name }}(self, payload):
        log.debug("listener was triggered")
        channel = self.bot.get_channel(payload.channel_id)
        if channel.name not in self.allowed_channels:
            log.debug("listener aborted, because: wrong channel")
            return
