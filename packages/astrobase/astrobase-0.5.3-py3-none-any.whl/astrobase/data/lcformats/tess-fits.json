{
    "fileglob": "*_lc.fits",
    "timecols": ["time", "time"],
    "magcols": ["sap.sap_flux", "pdc.pdcsap_flux"],
    "errcols": ["sap.sap_flux_err", "pdc.pdcsap_flux_err"],
    "magsarefluxes": true,
    "lcreader_module": "astrobase.astrotess",
    "lcreader_func": "read_tess_fitslc",
    "lcreader_kwargs": {"normalize": true},
    "lcnorm_module": "astrobase.astrotess",
    "lcnorm_func": "filter_tess_lcdict",
    "lcnorm_kwargs": null
}
